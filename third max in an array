import java.util.*;
import java.io.*;
public class Main{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int arr[]=new int[n];
    for(int i=0;i<arr.length;i++)
    {
      arr[i]=sc.nextInt();
    }
    Arrays.sort(arr);
   for(int j=arr.length-3;j>0;j--)
    {
      System.out.println(arr[j]);
     break;
    }
  }
}
